<%-include('header.ejs')%>
    <div class="container-fluid p-0 expenses">
        <div class="row">

            <div class="col-12" id="page_title">
                <h6 class="mt-1"> /Expense Report</h6>
            </div>

            <div class="col-12" id="page_title_big">
                <h1 class="mt-3">Expense Report</h1>
            </div>

            <div class="col-12">
                <form action="/get_rep" method="post">

                    <label>From Date</label>
                    <div class="col-12">
                        <input type="date" pattern="\d{4}-\d{2}-\d{2}" name="from_date" class="w-75 p-2" required>
                    </div>

                    <label class="mt-3">To Date</label>
                    <div class="col-12">
                        <input type="date" pattern="\d{4}-\d{2}-\d{2}" name="to_date" class="w-75 p-2" required>
                    </div>

                    <button class="btn btn-primary mt-4">View</button>
                </form>
            </div>
        </div>
    </div>
<%-include('footer.ejs')%>